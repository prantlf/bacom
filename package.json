{
  "name": "bacom",
  "version": "0.3.0",
  "description": "Basic library for writing web components.",
  "homepage": "https://github.com/prantlf/bacom#readme",
  "author": {
    "name": "Ferdinand Prantl",
    "email": "prantlf@gmail.com",
    "url": "http://prantl.tk"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/prantlf/bacom.git"
  },
  "bugs": {
    "url": "https://github.com/prantlf/bacom/issues"
  },
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/prantlf/bacom/blob/master/LICENSE"
    }
  ],
  "engines": {
    "node": ">=12"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.min.js",
  "types": "dist/index.d.ts",
  "browser": {
    "dist/index.js": "dist/index.umd.min.js"
  },
  "files": [
    "dist",
    "tools/style"
  ],
  "scripts": {
    "build:rollup": "rollup -c && mv dist3/src/* dist3 && rm -r dist3/memoize.d.ts dist3/src dist3/test",
    "prepare": "node tools/build prepare",
    "build": "node tools/build dist",
    "watch": "node tools/build watch",
    "precheck": "node tools/build test",
    "check": "node -r source-map-support/register test",
    "pretest": "node tools/build test",
    "test": "c8 node -r source-map-support/register test && NO_CONSTRUCTIBLE_STYLESHEETS=1 c8 --no-clean node -r source-map-support/register test/comp.test && c8 report -r text-summary && c8 check-coverage",
    "start": "python -m SimpleHTTPServer"
  },
  "c8": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/*",
      "dist/*"
    ],
    "reporter": [
      "lcov"
    ],
    "branches": 100,
    "lines": 100,
    "functions": 100,
    "statements": 100
  },
  "dependencies": {
    "@rollup/pluginutils": "^4.1.0"
  },
  "devDependencies": {
    "@prantlf/dom-lite": "^21.2.8",
    "@prantlf/esbuild": "^0.8.46",
    "c8": "^7.7.2",
    "source-map-support": "^0.5.19",
    "tehanu": "^0.0.3",
    "tehanu-repo-coco": "^0.0.2",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "keywords": [
    "web-components",
    "custom-elements"
  ]
}
